{
   "content_scripts": [ {
      "js": ["./groundify.js" ],
      "matches": [ "http://gist.github.com/*", "https://gist.github.com/*" ]
   } ],
   "description": "Display groundify on gist.github.com",
   "browser_action": {
    "default_icon": "./icon.png"
  },
   "manifest_version": 2,
   "name": "Groundify",
   "version": "0.1.0"
}
